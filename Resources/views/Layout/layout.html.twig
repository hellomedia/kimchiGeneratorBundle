    <!-- #BODY -->
    <!-- Possible Classes

        * 'smart-skin-{SKIN#}'
        * 'smart-rtl'         - Switch theme mode to RTL
        * 'menu-on-top'       - Switch to top navigation (no DOM change required)
        * 'hidden-menu'       - Hides the main menu but still accessable by hovering over left edge
        * 'fixed-header'      - Fixes the header
        * 'fixed-navigation'  - Fixes the main menu
        * 'fixed-ribbon'      - Fixes breadcrumb
        * 'fixed-footer'      - Fixes footer
        * 'container'         - boxed layout mode (non-responsive: will not work with fixed-navigation & fixed-ribbon)
    -->
    <body class="{% block body_classes '' %}">

        <!-- HEADER -->
        {% include 'GrizzlyLabAdminBundle:Layout:header.html.twig' %}
        <!-- END HEADER -->

        <!-- LEFT PANEL -->
        {% include 'GrizzlyLabAdminBundle:Layout:left-panel.html.twig' %}
        <!-- END LEFT-PANEL -->

        <!-- MAIN PANEL -->
        <div id="main" role="main">

            <!-- RIBBON -->
            {% include 'GrizzlyLabAdminBundle:Layout:ribbon.html.twig' %}
            <!-- END RIBBON -->

            <!-- MAIN CONTENT -->
            <div id="content">
                {% block flash_messages %}
                    <div id="flash-messages">
                    {% for type, flashMessages in app.session.flashbag.all() %}
                    {% for flashMessage in flashMessages %}
                    <div class="alert alert-{{ type }}">
                        {{ flashMessage | trans({}, 'flashMessages') }}
                    </div>
                    {% endfor %}
                    {% endfor %}
                </div>
                {% endblock %}
                {% block main_content %}
                {% endblock %}
            </div>
            <!-- END MAIN CONTENT -->

        </div>
        <!-- END MAIN PANEL -->

        <!-- PAGE FOOTER -->
        {% include 'GrizzlyLabAdminBundle:Layout:footer.html.twig' %}
